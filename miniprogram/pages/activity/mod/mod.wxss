@media (prefers-color-scheme: dark) {
  .ttt{
    filter: invert(80%) !important;
  }
  .inp {
    filter: invert(90%) !important;
  }
  .contentt {
    filter: invert(20%) !important;
  }
  .contentt > text{
    filter: invert(60%) !important;
  }
  .swtou{
    filter: invert(60%) !important;
  }
  .name{
    filter: invert(90%) !important;
  }
  .content{
    filter: invert(20%) !important;
  }
  button{
    filter: invert(30%) !important;
  }
}