@import "../../template/template.wxss";
page{
  background-color:#fffcfc;
}
.head{
    display: flex;
    line-height: 70rpx;
    box-shadow:0px 0px 10px #e2e2e2;
    padding-bottom: 10rpx;
    background-color: #fff;
}
.head-time{
  margin-left: 25rpx;
}
.head-title{
    flex: 1;
    text-align: right;
    margin-right: 25rpx;
}
.head-title view{
    display: inline-block;
    font-size:25rpx;
}
.head-title view:nth-child(1){
    margin-right:30rpx;
}
.weui-input{
    font-size:30rpx;
    font-weight:600;
}
.main{
    margin:10rpx 26rpx 30rpx 20rpx;
}
.wx-isclass{
  text-align:center;
  font-size:40rpx;
  color:#1cbbb4;
  margin-top:30rpx;
}

.list{
  margin-bottom: 30rpx;
}
.icon-open{
  margin-left: 20rpx;
}
