page {
  background-color: rgb(255, 255, 255);
}

.section__title {

  margin-bottom: 20rpx;
  height: 70rpx;
}

.picker {
  height: 80rpx;
}

.section {

  margin: 50rpx 0;

  font-size: 30rpx;

}

.wx-remind-detail {
  justify-content: center;
  align-items: center;
  display: flex;
}

.wx-remind-detail text {

  padding: 14rpx 20rpx;
  border: 4rpx solid coral;
  color: coral;
  border-radius: 20rpx;
}

/*-----------------------------遮罩层样式-------------------------------------  */
.shade {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 102;
  background-color: rgba(0, 0, 0, 0.8);
  opacity: 0.5;
  overflow: hidden;
}

/*----------------------------面板样式-----------------------------------------  */
.cont {
  width: 600rpx;
  height: 400rpx;
  z-index: 103;
  overflow: hidden;
  position: fixed;
  top: 20%;
  left: 60rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
  border: 1rpx solid greenyellow;
  background-color: white;
}

.tit {
  color: coral;
}

.wx-isclass {
  width: 730rpx;
  text-align: center;
  font-size: 40rpx;
  color: #1cbbb4;
  margin-top: 20rpx;
}

.flex-item {
  width: 96rpx;
  height: 100px;
}

.kcb-item {
  position: absolute;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 5px;
}

.smalltext {
  font-size: 80%;
  color: #fff;
  padding: 0 2rpx;
}

.top {
  display: flex;
  padding-top:20rpx;
  flex-direction: row;
}
.ttt{
  font-size: 35rpx;
}
.text {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 6;
}


.after-left {
  text-align: center;
  font-size: 8.5pt;
  justify-content: center;
  align-items: center;
  color: #aaa;
  
}

.after-left1 {
  text-align: center;
  font-size: 7pt;
  justify-content: center;
  align-items: center;
  color: #aaa;
  
}

.timetable {
  height: 1200rpx;
  width: 730rpx;
  display: flex;
}
.timetable-item {
  position: absolute;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 3px;
}

.after-left-strong {
  color: #73b4ef;
  font-weight: bold;
}


.top-text {
  justify-content: center;
  align-items: center;
  text-align: center;
  flex-grow: 1;
  width: 100rpx;
  height: 75rpx;
  font-size: 11pt;
  color: #4d4d4d;
}

.top-text-strong {
  color: #73b4ef;
  font-weight: bold;
}

.scroll {
  /* height: 1300rpx; */
  /* z-index: 9999999; */
  /* height: 100%; */
  position: absolute;
  top:7%;
  
}

.left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  width: 50rpx;
  height: 100rpx;
  font-size: 12pt;
  color: #4d4d4d;
}

.nav-head {
  display: flex;
  position:fixed;
  width: 100%;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 7%;
  z-index: 10000;
  border-bottom: 1rpx solid #cacbcc;
  background-color: #fff;
}
.week {
  height: 35rpx;
  width: 50rpx;
  font-size: 9pt;
  justify-content: center;
  display: flex;
  align-items: center;
  margin-top: 15rpx;
}
.week-selector {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 330rpx;
}

.img-button {
  display: inline-block;
}

.img-button image {
  width: 100%;
  height: 100%;
}

.add-course-btn {
  width: 52rpx;
  height: 52rpx;
  margin-left: 20rpx;
}

.pre-week-btn {
  width: 20rpx;
  height: 39rpx;
}

.cur-week {
  text-align: center;
  line-height: 1.5;

}

.cur-week .week {
  font-size: 38rpx;
  color: #18191A;
}

.cur-week .title {
  font-size: 28rpx;
  color: #7A7D80;
  max-width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
}

.next-week-btn {
  width: 20rpx;
  height: 39rpx;
}

.feedback-btn {
  width: 50rpx;
  height: 48rpx;
  margin-right: 20rpx;
}

.pd2 {
  height: 100%;

  justify-content: space-between;
  padding: 88% 0;
  box-sizing: border-box;
}

.round-click {
  height: 120rpx;
  width: 120rpx;
  background-color: #bc3e49;
  border-radius: 100%;
  position: fixed;
  bottom: 50rpx;
  right: 25rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}

.round-click text {
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}

.month{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  width: 35rpx;
}
.month text{
  min-width: 35rpx;
  text-align: center;
}